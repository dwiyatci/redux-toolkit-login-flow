{
  "name": "redux-toolkit-login-flow",
  "version": "2.3.0",
  "private": true,
  "description": "A potentially reusable formalized login flow pattern for crafting web development project",
  "keywords": [
    "login flow",
    "formalized pattern",
    "redux toolkit app",
    "react app",
    "react router app",
    "web craftmanship"
  ],
  "bugs": "https://github.com/dwiyatci/redux-toolkit-login-flow/issues",
  "license": "SEE LICENSE IN LICENSE.txt",
  "author": "Glenn Dwiyatcita <gmd@email.com> (https://tiny.cc/dwiyatci)",
  "repository": {
    "type": "git",
    "url": "https://github.com/dwiyatci/redux-toolkit-login-flow.git"
  },
  "scripts": {
    "cix": "pnpm install:clean",
    "install:clean": "pnpm dlx rimraf node_modules && pnpm install",
    "rd": "pnpm reinstall:dedupe",
    "reinstall:dedupe": "pnpm dedupe && pnpm install",
    "upgr": "pnpm upgrade --recursive --latest --interactive",
    "start": "vite",
    "test": "vitest",
    "preview": "vite preview",
    "build": "vite build",
    "format": "prettier --write \"{**/*,*}.{js,ts,jsx,tsx,json,yml,yaml,xml,html,css,less,sass,scss,md,sh,graphql,gql}\"",
    "format:check": "prettier --list-different \"{**/*,*}.{js,ts,jsx,tsx,json,yml,yaml,xml,html,less,sass,scss,md,sh,graphql,gql}\"",
    "lint": "pnpm lint:report-warnings --quiet",
    "lint:report-warnings": "eslint ./src",
    "flow": "flow"
  },
  "dependencies": {
    "@reduxjs/toolkit": "2.8.2",
    "react": "19.1.1",
    "react-dom": "19.1.1",
    "react-redux": "9.2.0",
    "react-router": "7.7.1"
  },
  "devDependencies": {
    "@babel/preset-flow": "7.27.1",
    "@eslint/js": "9.32.0",
    "@testing-library/jest-dom": "6.6.4",
    "@testing-library/react": "16.3.0",
    "@vitejs/plugin-react": "4.7.0",
    "babel-plugin-syntax-hermes-parser": "0.31.0",
    "eslint": "9.32.0",
    "eslint-plugin-ft-flow": "3.0.11",
    "eslint-plugin-react": "7.37.5",
    "flow-bin": "0.278.0",
    "globals": "16.3.0",
    "hermes-eslint": "0.31.0",
    "jsdom": "26.1.0",
    "prettier": "3.6.2",
    "vite": "7.0.6",
    "vitest": "3.2.4"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "volta": {
    "node": "22.18.0"
  }
}
